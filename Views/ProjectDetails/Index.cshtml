            <th>
                @Html.DisplayNameFor(model => model.RResponsible)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Protocol)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ProtocolTime)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ApprovedTime)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CreateDate)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
