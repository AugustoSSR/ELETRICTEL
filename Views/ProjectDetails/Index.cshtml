@model IEnumerable<ELETRICTEL.Models.ProjectDetails>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Projects)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ProjectKM)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ProjectPostes)
